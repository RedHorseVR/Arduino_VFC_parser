set(/*);// 
set(Switch statement);// 
set(Demonstrates the use of a switch statement. The switch statement allows you);// 
set(to choose from among a set of discrete values of a variable. It's like a);// 
set(series of if statements.);// 
set(To see this sketch in action, put the board and sensor in a well-lit room,);// 
set(open the Serial Monitor, and move your hand gradually down over the sensor.);// 
set(The circuit:);// 
set(- photoresistor from analog in 0 to +5V);// 
set(- 10K resistor from analog in 0 to ground);// 
set(created 1 Jul 2009);// 
set(modified 9 Apr 2012);// 
set(by Tom Igoe);// 
set(This example code is in the public domain.);// 
set(https:);// www.arduino.cc/en/Tutorial/BuiltInExamples/SwitchCase
set(*/);// 
set();//  these constants won't change. They are the lowest and highest readings you
set();//  get from your sensor:
set(const int sensorMin = 0;);//  sensor minimum, discovered through experiment
set(const int sensorMax = 600;);//  sensor maximum, discovered through experiment

input(void setup());// beginfunc
set({);// 
set();//  initialize serial communication:
output(Serial.begin(9600););// 
end(});// endfunc

input(void loop());// beginfunc
set({);// 
set();//  read the sensor:
set(int sensorReading = analogRead(A0););// 
set();//  map the sensor range to a range of four options:
set(int range = map(sensorReading, sensorMin, sensorMax, 0, 3););// 
set();//  do something different depending on the range value:
branch(switch (range));// beginswitch
path({);//
path(case 0:);//  your hand is on the sensor
output(Serial.println("dark"););// 
end(break;);// 
path(case 1:);//  your hand is close to the sensor
output(Serial.println("dim"););// 
end(break;);// 
path(case 2:);//  your hand is a few inches from the sensor
output(Serial.println("medium"););// 
end(break;);// 
path(case 3:);//  your hand is nowhere near the sensor
output(Serial.println("bright"););// 
end(break;);// 
bend(});// endswitch
set(delay(1););//  delay in between reads for stability
end(});// endfunc



